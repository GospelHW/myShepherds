        <TextView
            android:id="@+id/area"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:gravity="right|center_vertical" />
    </LinearLayout>

    <LinearLayout
